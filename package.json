{
  "name": "l10n-tools",
  "version": "3.0.9",
  "description": "Localization tools",
  "type": "module",
  "scripts": {
    "build": "swc src -d dist -s",
    "watch": "swc -w src -d dist -s",
    "schema": "typescript-json-schema --required --noExtraProps --strictNullChecks tsconfig.json L10nConf > l10nrc.schema.json",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run tsc && npm test"
  },
  "author": "Eungkyu Song <eungkyu@gmail.com>",
  "license": "MIT",
  "repository": "github:TPC-Internet/l10n-tools",
  "bin": {
    "l10n": "dist/l10n.js"
  },
  "dependencies": {
    "@lokalise/node-api": "^9.8.1",
    "ajv": "^8.12.0",
    "command-exists": "^1.2.9",
    "commander": "^10.0.1",
    "cosmiconfig": "^8.2.0",
    "fast-xml-parser": "^4.2.4",
    "gettext-parser": "^7.0.1",
    "glob": "^10.2.7",
    "google-auth-library": "^8.8.0",
    "googleapis": "^118.0.0",
    "he": "^1.2.0",
    "http-shutdown": "^1.2.2",
    "i18n-strings-files": "^2.0.0",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^6.1.5",
    "npmlog": "^7.0.1",
    "open": "^9.1.0",
    "p-queue": "^7.3.4",
    "php-parser": "^3.1.5",
    "plist": "^3.0.6",
    "shelljs": "^0.8.5",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/command-exists": "^1.2.0",
    "@types/he": "^1.2.0",
    "@types/jest": "^29.5.2",
    "@types/jsonfile": "^6.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.17",
    "@types/npmlog": "^4.1.4",
    "@types/plist": "^3.0.2",
    "@types/shelljs": "^0.8.12",
    "chokidar": "^3.5.3",
    "jest": "^29.5.0",
    "typescript-json-schema": "^0.57.0"
  }
}
