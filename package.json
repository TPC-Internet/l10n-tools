{
  "name": "l10n-tools",
  "version": "4.1.2",
  "description": "Localization tools",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "swc src -d dist -s",
    "watch": "swc -w src -d dist -s",
    "schema": "typescript-json-schema --required --noExtraProps --strictNullChecks tsconfig.json L10nConf > l10nrc.schema.json",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run tsc && npm test"
  },
  "author": "Eungkyu Song <eungkyu@gmail.com>",
  "license": "MIT",
  "repository": "github:TPC-Internet/l10n-tools",
  "bin": {
    "l10n": "dist/l10n.js"
  },
  "dependencies": {
    "@lokalise/node-api": "^12.1.0",
    "ajv": "^8.12.0",
    "command-exists": "^1.2.9",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "fast-xml-parser": "^4.3.2",
    "gettext-parser": "^7.0.1",
    "glob": "^10.3.10",
    "google-auth-library": "^9.4.1",
    "googleapis": "^130.0.0",
    "he": "^1.2.0",
    "http-shutdown": "^1.2.2",
    "i18n-strings-files": "^2.0.0",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^6.1.12",
    "npmlog": "^7.0.1",
    "open": "^10.0.3",
    "p-queue": "^8.0.1",
    "php-parser": "^3.1.5",
    "plist": "^3.1.0",
    "shelljs": "^0.8.5",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.71",
    "@swc/jest": "^0.2.29",
    "@types/command-exists": "^1.2.3",
    "@types/he": "^1.2.3",
    "@types/jest": "^29.5.11",
    "@types/jsonfile": "^6.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.5",
    "@types/npmlog": "^7.0.0",
    "@types/plist": "^3.0.5",
    "@types/shelljs": "^0.8.15",
    "chokidar": "^3.5.3",
    "jest": "^29.7.0",
    "typescript-json-schema": "^0.62.0"
  }
}
